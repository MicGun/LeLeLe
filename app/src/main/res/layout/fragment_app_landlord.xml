<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Button
        android:id="@+id/button_electricity_landlord"
        style="@style/ProfileOrderButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@+id/button_message_landlord"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_electricity_landlord"
        app:layout_constraintEnd_toEndOf="@+id/button_electricity_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_electricity_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_electricity_landlord">

        <ImageView
            android:id="@+id/image_view_electricity_editor_icon"
            style="@style/ApplicationIconDimension"
            android:tint="@color/brown_500708"
            app:layout_constraintBottom_toTopOf="@+id/text_view_electricity_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread"
            app:srcCompat="@drawable/ic_electricity" />

        <TextView
            android:id="@+id/text_view_electricity_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_electricity_edit"
            android:fontFamily="@font/noto_sans"
            android:textSize="14sp"
            android:textColor="@color/brown_500708"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_electricity_editor_icon" />

    </android.support.constraint.ConstraintLayout>


    <Button
        android:id="@+id/button_message_landlord"
        style="@style/ProfileOrderButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/button_electricity_landlord"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@+id/button_contract_landlord"
        app:layout_constraintStart_toEndOf="@+id/button_electricity_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_electricity_landlord" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_message_landlord"
        app:layout_constraintEnd_toEndOf="@+id/button_message_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_message_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_message_landlord">

        <ImageView
            android:id="@+id/image_view_message_icon"
            style="@style/ApplicationIconDimension"
            android:tint="@color/brown_500708"
            app:layout_constraintBottom_toTopOf="@+id/text_view_message_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread"
            app:srcCompat="@drawable/ic_message" />

        <TextView
            android:id="@+id/text_view_message_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_message"
            android:fontFamily="@font/noto_sans"
            android:textSize="14sp"
            android:textColor="@color/brown_500708"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_message_icon" />

    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/button_contract_landlord"
        style="@style/ProfileOrderButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/button_electricity_landlord"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_message_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_electricity_landlord" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_contract_landlord"
        app:layout_constraintEnd_toEndOf="@+id/button_contract_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_contract_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_contract_landlord">

        <ImageView
            android:id="@+id/image_view_contract_icon"
            style="@style/ApplicationIconDimension"
            android:tint="@color/brown_500708"
            app:layout_constraintBottom_toTopOf="@+id/text_view_contract_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread"
            app:srcCompat="@drawable/ic_action_contract" />

        <TextView
            android:id="@+id/text_view_contract_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_contract"
            android:fontFamily="@font/noto_sans"
            android:textSize="14sp"
            android:textColor="@color/brown_500708"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_contract_icon" />

    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/button_reservation_landlord"
        style="@style/ProfileOrderButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/button_electricity_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_electricity_landlord"
        app:layout_constraintTop_toBottomOf="@+id/button_electricity_landlord" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_reservation_landlord"
        app:layout_constraintEnd_toEndOf="@+id/button_reservation_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_reservation_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_reservation_landlord">

        <ImageView
            android:id="@+id/image_view_reservation_icon"
            style="@style/ApplicationIconDimension"
            android:tint="@color/brown_500708"
            app:layout_constraintBottom_toTopOf="@+id/text_view_reservation_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread"
            app:srcCompat="@drawable/ic_calendar" />

        <TextView
            android:id="@+id/text_view_reservation_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_reservation"
            android:fontFamily="@font/noto_sans"
            android:textSize="14sp"
            android:textColor="@color/brown_500708"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_reservation_icon" />

    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/button_group_list_landlord"
        style="@style/ProfileOrderButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/button_electricity_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_electricity_landlord"
        app:layout_constraintTop_toBottomOf="@+id/button_reservation_landlord" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_group_list_landlord"
        app:layout_constraintEnd_toEndOf="@+id/button_group_list_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_group_list_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_group_list_landlord">

        <ImageView
            android:id="@+id/image_view_group_list_icon"
            style="@style/ApplicationIconDimension"
            android:tint="@color/brown_500708"
            app:layout_constraintBottom_toTopOf="@+id/text_view_group_list_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread"
            app:srcCompat="@drawable/ic_action_connection" />

        <TextView
            android:id="@+id/text_view_group_list_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_groups_list"
            android:fontFamily="@font/noto_sans"
            android:textSize="14sp"
            android:textColor="@color/brown_500708"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_group_list_icon" />

    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/button_garbage_truck_landlord"
        style="@style/ProfileOrderButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/button_reservation_landlord"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/button_message_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_message_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_reservation_landlord" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_garbage_truck_landlord"
        app:layout_constraintEnd_toEndOf="@+id/button_garbage_truck_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_garbage_truck_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_garbage_truck_landlord">

        <ImageView
            android:id="@+id/image_view_garbage_truck_icon"
            style="@style/ApplicationIconDimension"
            android:tint="@color/brown_500708"
            app:layout_constraintBottom_toTopOf="@+id/text_view_garbage_truck_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread"
            app:srcCompat="@drawable/ic_action_garbage_truck" />

        <TextView
            android:id="@+id/text_view_garbage_truck_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_garbage_truck"
            android:fontFamily="@font/noto_sans"
            android:textSize="14sp"
            android:textColor="@color/brown_500708"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_garbage_truck_icon" />

    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/button_room_list_landlord"
        style="@style/ProfileOrderButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/button_reservation_landlord"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/button_contract_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_contract_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_reservation_landlord" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_room_list_landlord"
        app:layout_constraintEnd_toEndOf="@+id/button_room_list_landlord"
        app:layout_constraintStart_toStartOf="@+id/button_room_list_landlord"
        app:layout_constraintTop_toTopOf="@+id/button_room_list_landlord">

        <ImageView
            android:id="@+id/image_view_room_list_icon"
            style="@style/ApplicationIconDimension"
            android:tint="@color/brown_500708"
            app:layout_constraintBottom_toTopOf="@+id/text_view_room_list_app"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="spread"
            app:srcCompat="@drawable/ic_action_bed" />

        <TextView
            android:id="@+id/text_view_room_list_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_room_list"
            android:fontFamily="@font/noto_sans"
            android:textSize="14sp"
            android:textColor="@color/brown_500708"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_room_list_icon" />

    </android.support.constraint.ConstraintLayout>

    <ProgressBar
        android:id="@+id/progress_bar_app_landlord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:theme="@style/ProgressTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>